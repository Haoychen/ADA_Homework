jackknife <- function(func, data){
estimator.sum <- 0
for(i in 1:length(data)){
estimator.sum <- estimator.sum + func(data[-i])
}
estimator.bias <- (length(data) - 1) * (estimator.sum / length(data) - func(data))
return(estimator.bias)
}
